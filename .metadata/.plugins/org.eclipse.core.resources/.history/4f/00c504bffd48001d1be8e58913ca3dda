package br.com.merge.model;

import java.util.List;

import javax.xml.bind.annotation.XmlRootElement;

/**
 *Classe que repesenta uma Empresa
 *@author Henrique Cesar
 *@author Dennys Nascimenro 
 *@author Luan Reis
 *@author Gustavo Fonseca
 *@author Rodrigo Machado
 *
 */

@XmlRootElement
public class Empresa {
	
	/**
	 * Armazena a descrição, nome, endereco, razao social, nome do responsavel, sigla do Estado, nome do Estado, nome 
	 * da cidade e descrição do email da empresa
	 */
	private String dsEmpr, nmEmpr, dsEndereco, dsRazaoSocial, nmResponsavel, sgEstado, nmEstado, nmCidade, dsEmail;  
	
	/**
	 * Armazena o numero do endereco, numero do cnpj, nrcep
	 */
	private Integer nrEndereco, nrCnpj, nrCep;
	
	/**
	 * Armazena coleção de contatos
	 */
	private List<ContatoEmpresa> contato;
	
	/**
	 * Construtor padrão
	 */
	public Empresa() {
	}

	/**
	 * Construtor que recebe a descrição, nome, endereco, razao social, nome do responsavel, sigla do Estado, nome do Estado, nome 
	 * da cidade e descrição do email, o numero do endereco, numero do cnpj, nrcep e coleção de contatos da empresa
	 * @param descrição da Empresa
	 * @param nome da Empresa
	 * @param descrição do Endereco da empresa
	 * @param descrição da RazaoSocial da empresa
	 * @param nome do Responsavel da empresa
	 * @param sigla do Estado da empresa
	 * @param nome do Estado da empresa
	 * @param nome da Cidade da empresa
	 * @param descrição do Email da empresa 
	 * @param numero do Endereco da empresa
	 * @param numero dp Cnpj empresa
	 * @param numero do Cep da empresa
	 * @param contato da empresa
	 */
	public Empresa(String dsEmpr, String nmEmpr, String dsEndereco, String dsRazaoSocial, String nmResponsavel,
			String sgEstado, String nmEstado, String nmCidade, String dsEmail, Integer nrEndereco, Integer nrCnpj,
			Integer nrCep, List<ContatoEmpresa> contato) {
		this.dsEmpr = dsEmpr;
		this.nmEmpr = nmEmpr;
		this.dsEndereco = dsEndereco;
		this.dsRazaoSocial = dsRazaoSocial;
		this.nmResponsavel = nmResponsavel;
		this.sgEstado = sgEstado;
		this.nmEstado = nmEstado;
		this.nmCidade = nmCidade;
		this.dsEmail = dsEmail;
		this.nrEndereco = nrEndereco;
		this.nrCnpj = nrCnpj;
		this.nrCep = nrCep;
		this.contato = contato;
	}

	/**
	 * Retorna a descrição da empresa
	 * @return a descrição da empresa
	 */
	public String getDsEmpresa() {
		return dsEmpr;
	}

	/**
	 * Altera a descrição da empresa
	 * @param descrição da Empresa
	 */
	public void setDsEmpresa(String dsEmpr) {
		this.dsEmpr = dsEmpr;
	}

	/**
	 * Retorna o nome da empresa
	 * @return o nome da empresa
	 */
	public String getNmEmpresa() {
		return nmEmpr;
	}

	/**
	 * Altera o nome da empresa
	 * @param o nome da Empresa
	 */
	public void setNmEmpresa(String nmEmpr) {
		this.nmEmpr = nmEmpr;
	}

	/**
	 * Retorna a descricao do endereco
	 * @return a descricao do endereco
	 */
	public String getDsEndereco() {
		return dsEndereco;
	}

	/**
	 * Altera a descrição do endereco
	 * @param descricao do Endereco
	 */
	public void setDsEndereco(String dsEndereco) {
		this.dsEndereco = dsEndereco;
	}

	/**
	 * Retorna a descrição da razão social
	 * @return descrição da razão social
	 */
	public String getDsRazaoSocial() {
		return dsRazaoSocial;
	}

	/**
	 * Altera a razão social
	 * @param descrição da razão social
	 */
	public void setDsRazaoSocial(String dsRazaoSocial) {
		this.dsRazaoSocial = dsRazaoSocial;
	}

	/**
	 * Retorna o nome do responsável
	 * @return nome do responsavel
	 */
	public String getNmResponsavel() {
		return nmResponsavel;
	}

	/**
	 * Altera o nome do responsável da empresa
	 * @param nome do responsável da empresa
	 */
	public void setNmResponsavel(String nmResponsavel) {
		this.nmResponsavel = nmResponsavel;
	}

	/**
	 * Retorna a sigla do estado da empresa
	 * @return sigla do estado da empresa
	 */
	public String getSgEstado() {
		return sgEstado;
	}

	/**
	 *Altera a sigla do estado da empresa
	 * @param sigla do estado da empresa
	 */
	public void setSgEstado(String sgEstado) {
		this.sgEstado = sgEstado;
	}

	/**
	 * Retorna o nome do estado da empresa
	 * @return o nome do estado da empresa
	 */
	public String getNmEstado() {
		return nmEstado;
	}

	/**
	 *Altera o nome do estado da empresa
	 * @param o nome do estado da empresa
	 */
	public void setNmEstado(String nmEstado) {
		this.nmEstado = nmEstado;
	}

	/**
	 * Retorna o nome da cidade da empresa
	 * @return o nome do estado da empresa
	 */
	public String getNmCidade() {
		return nmCidade;
	}

	/**
	 * Altera o nome da cidade da empresa
	 * @param nome da cidade da empresa
	 */
	public void setNmCidade(String nmCidade) {
		this.nmCidade = nmCidade;
	}

	/**
	 * Retorna a descrição do email da empresa
	 * @return a descrição do emai da empresa
	 */
	public String getDsEmail() {
		return dsEmail;
	}

	/**
	 * Altera a descrição do email da empresa
	 * @param a descrição do email da empresa
	 */
	public void setDsEmail(String dsEmail) {
		this.dsEmail = dsEmail;
	}

	/**
	 * Retorna o endereco da empresa
	 * @return o endereco da empresa
	 */
	public Integer getNrEndereco() {
		return nrEndereco;
	}

	/**
	 * Altera o numero do endereco da empresa
	 * @param o numero do endereco da empresa
	 */
	public void setNrEndereco(Integer nrEndereco) {
		this.nrEndereco = nrEndereco;
	}

	/**
	 * Retorna número do cnpj da empresa
	 * @return onúmero do cnpj da empresa
	 */
	public Integer getNrCnpj() {
		return nrCnpj;
	}

	/**
	 * Altera o numero do cnpj da empresa
	 * @param o numero do cnpj da empresa
	 */
	public void setNrCnpj(Integer nrCnpj) {
		this.nrCnpj = nrCnpj;
	}

	/**
	 * Retorna número do cep da empresa
	 * @return onúmero do cep da empresa
	 */
	public Integer getNrCep() {
		return nrCep;
	}

	/**
	 * Altera o numero do cep da empresa
	 * @param o numero do cep da empresa
	 */
	public void setNrCep(Integer nrCep) {
		this.nrCep = nrCep;
	}

	/**
	 * Retorna a coleção de contatos da empresa
	 * @return coleção de contatos da empresa
	 */
	public List<ContatoEmpresa> getContato() {
		return contato;
	}

	/**
	 * Altera a coleção de contatos da empresa
	 * @param a coleção de contatos da empresa
	 */
	public void setContato(List<ContatoEmpresa> contato) {
		this.contato = contato;
	}
}